{
  "name": "flowershow",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:up": "bash scripts/dev-up.sh",
    "dev:up:all": "bash scripts/dev-up.sh --all",
    "dev:down": "docker compose --profile stripe --profile github --profile search down",
    "dev:nuke": "docker compose --profile stripe --profile github --profile search down -v",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "turbo run format",
    "format:write": "turbo run format:write",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.1",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^24.9.1",
    "husky": "^8.0.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.4",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.12.1",
  "pnpm": {
    "patchedDependencies": {
      "gray-matter@4.0.3": "apps/flowershow/patches/gray-matter@4.0.3.patch"
    },
    "overrides": {
      "@types/react": "19.2.2",
      "@types/react-dom": "19.2.2",
      "d3-color": ">=3.1.0",
      "dset": ">=3.1.4",
      "dompurify": ">=3.2.4",
      "lodash": ">=4.17.23",
      "lodash-es": ">=4.17.23",
      "preact": ">=10.28.2",
      "qs": ">=6.14.2",
      "glob": ">=10.5.0",
      "fast-xml-parser": ">=5.3.4",
      "@isaacs/brace-expansion": ">=5.0.1",
      "vega-functions": ">=6.1.1",
      "vega-selections": ">=6.1.2",
      "js-yaml": ">=4.1.1",
      "mdast-util-to-hast": ">=13.2.1",
      "diff": ">=5.2.2"
    }
  }
}
