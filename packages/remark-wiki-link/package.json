{
  "name": "@flowershow/remark-wiki-link",
  "version": "3.4.0",
  "description": "Parse and render wiki-style links, especially Obsidian style links.",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --sourcemap",
    "lint": "biome lint .",
    "test": "vitest run",
    "release": "changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowershow/remark-wiki-link.git"
  },
  "keywords": [
    "remark",
    "remark-plugin",
    "markdown",
    "obsidian"
  ],
  "author": "Flowershow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flowershow/remark-wiki-link/issues"
  },
  "homepage": "https://github.com/flowershow/remark-wiki-link#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "github-slugger": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.2",
    "micromark-util-character": "^2.0.0",
    "micromark-util-symbol": "^2.0.1"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "mdast-util-from-markdown": "^2.0.2",
    "micromark": "^4.0.2",
    "micromark-util-types": "^2.0.2",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "tsup": "^8.5.0",
    "unified": "^11.0.5",
    "unist-util-select": "^5.1.0",
    "unist-util-visit": "^5.1.0",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}